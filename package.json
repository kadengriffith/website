{
  "name": "bytewave",
  "version": "1.0.2",
  "description": "A progressive web app made by Kaden Griffith",
  "repository": "https://github.com/kadengriffith/bytewave.git",
  "main": "app.js",
  "kbrew-assets-url": "_dist",
  "scripts": {
    "start": "npm run build-index && webpack-dev-server --mode development --open --hot && npm run replace-index-file",
    "start-relay": "npm run build-index && webpack-dev-server --host 192.168.116.114 --mode development --open --hot && npm run replace-index-file",
    "build": "echo Initiating Build... && npm run build-index && npm run build-app-projects && webpack -p --optimize-minimize && npm run replace-index-file && npm run replace-dev-build && npm run build-dist",
    "build-dist": "cp ./_server/pwa/manifest.json ./_dist/manifest.json && cp ./_server/pwa/service-worker.js ./_dist/service-worker.js && npm run build-list",
    "build-list": "node ./_server/_util/asset.rebuild.js",
    "build-app-projects": "npm run backup-dev-build",
    "build-index": "npm run backup-index-file && node ./_public/html/html.generate.js",
    "backup-dev-build": "cp ./_public/app-full.js ./_server/_util/_backups/_backup.app-full.js",
    "backup-index-file": "cp ./_public/html/index.html ./_server/_util/_backups/_backup.index.html",
    "replace-dev-build": "rm ./_public/app-full.js && cp ./_server/_util/_backups/_backup.app-full.js ./_public/app-full.js && rm ./_server/_util/_backups/_backup.app-full.js",
    "replace-index-file": "rm ./_public/html/index.html && cp ./_server/_util/_backups/_backup.index.html ./_public/html/index.html && rm ./_server/_util/_backups/_backup.index.html",
    "connect": "firebase login && firebase use --add",
    "deploy": "yarn build && firebase deploy"
  },
  "author": "Kaden Griffith",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fs": "0.0.1-security",
    "html-loader": "^0.5.5",
    "html-minifier": "^3.5.18",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "path": "^0.12.7",
    "postcss": "^6.0.23",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.2.2",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "webpack-require-http": "^0.4.3"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "firebase": "^5.3.0",
    "intersection-observer": "^0.5.0",
    "kbrew_hypertxt": "^1.0.4",
    "node-sass": "^4.9.2"
  }
}